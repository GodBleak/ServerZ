{
    "compilerOptions": {
        "strict": true,
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "importsNotUsedAsValues": "remove",
        "outDir": "dist",
        "rootDir": "src",
        "types": ["node"]
    }
}
